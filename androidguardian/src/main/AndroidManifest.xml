<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.virtualmanila.guardianportallister.sgmy">

	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

	<application
			android:name="com.virtualmanila.guardianportallister.sgmy.MyApplication"
			android:allowBackup="false"
			android:icon="@drawable/ic_launcher"
			android:label="@string/app_name"
			android:theme="@style/AppTheme">
		<activity android:name="com.virtualmanila.guardianportallister.sgmy.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>

		<service
				android:name="com.virtualmanila.guardianportallister.sgmy.service.GetGPListService"
				android:exported="false"/>

		<meta-data
				android:name="com.google.android.gms.version"
				android:value="@integer/google_play_services_version"/>

	</application>

</manifest>